---
import AlbumPhotos from '../components/AlbumPhotos.astro';
import AlbumPhotosSmall from '../components/AlbumPhotosSmall.astro';
import Heading2 from '../components/Heading2.astro';
import AlbumContainerLarge from '../layouts/Album/AlbumContainer-Large.astro';
import AlbumContainerMedium from '../layouts/Album/AlbumContainer-Medium.astro';
import AlbumContainerSmall from '../layouts/Album/AlbumContainer-Small.astro';
import AlbumContainerExtraSmall from '../layouts/Album/AlbumContainer-ExtraSmall.astro';
import AlbumContainer from '../layouts/Album/AlbumContainer.astro';
import Container from '../layouts/Container.astro';
import Layout from '../layouts/Layout.astro';
import Jumbotron from '../components/Jumbotron.astro';
---

<Layout title="Sponsor">
	<main>
		<Container>
            <AlbumContainer>
                <Heading2 text="Platinum Sponsors"/>
				<AlbumContainerLarge>
					<AlbumPhotos 
						src="/sponsors/bankindonesia2.png"
						alt="Bank Indonesia"
					/>
					<AlbumPhotos 
						src="/sponsors/angkasapura2.png"
						alt="Angkasa Pura 2"
					/>
					<AlbumPhotos 
						src="/sponsors/ptpii.png"
						alt="PT PII"
					/>
					<AlbumPhotos 
						src="/sponsors/indosatooredoo.png"
						alt="Indosat Ooredeoo"
					/>
					<AlbumPhotos 
						src="/sponsors/klinikaku2.png"
						alt="Klinik Aku"
					/>
				</AlbumContainerLarge>
			</AlbumContainer>
        </Container>
		<Container>
            <AlbumContainer>
                <Heading2 text="Silver Sponsors"/>
                <AlbumContainerMedium>
                    <AlbumPhotos 
						src="/sponsors/kapsriyadi.png"
						alt="KAP Sriyadi, Elly, Sugeng, dan Rekan"
					/>
                    <AlbumPhotos 
						src="/sponsors/sayurbox.png"
						alt="Sayurbox"
					/>
                </AlbumContainerMedium>
            </AlbumContainer>
		</Container>
		<Container>
            <AlbumContainer>
                <Heading2 text="Bronze Sponsors"/>
				<AlbumContainerSmall>
                    <AlbumPhotos 
                    src="/sponsors/wardah.png"
                    alt="Wardah Cosmetics"
					/>
				</AlbumContainerSmall>
			</AlbumContainer>
		</Container>
        <div style="height: 5vh;"></div>
		<Container>
            <AlbumContainer>
                <Heading2 text="Media Partners"/>
				<AlbumContainerExtraSmall>
                    <AlbumPhotosSmall 
                    src="/mediapartner/eventcampus2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/eventcenter2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/eventdetect2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/eventkampus2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/eventnyamahasiswa2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/ikutevent2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/infolomba.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/infolombaekonomi2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/lombamahasiswa.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/olimpiadeupdate2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/pointkampus2.png"
                    alt="Bank Indonesia"
					/>
                    <AlbumPhotosSmall 
                    src="/mediapartner/sejutacita.png"
                    alt="Bank Indonesia"
					/>
				</AlbumContainerExtraSmall>
			</AlbumContainer>
		</Container>
	</main>
</Layout>