---
export interface Props {
    src: string;
    alt: string;
}

const { src, alt } = Astro.props
---
<div class="col">
    <div class="d-flex justify-content-center">
        <img
            src={ src }
            alt= { alt }
            width="70%"
            height="70%"
            class="lazy"
        />
    </div>
</div>
