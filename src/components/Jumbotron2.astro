---
export interface Props {
    srcdesktop: string;
    srcmobile: string;
    alt: string;
}

const {srcdesktop, srcmobile, alt} = Astro.props
---
<picture>
    <source media="(min-width:650px)" srcset={srcdesktop}>
    <source media="(max-width:465px)" srcset={srcmobile}>
    <img class="img-fluid lazy" src={srcmobile} alt={alt} style="width:auto;">
  </picture>